<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.nlog-project.org/schemas/NLog.xsd NLog.xsd"
      autoReload="true"
      throwExceptions="true"
      internalLogLevel="Trace" internalLogFile="c:\temp\nlog-internal.log" >
  <targets>
    <target xsi:type="Kafka"
       name="Kafka"
       topic="storm_troper"
       layout="Put MoService ${longdate} 1 level=${level}">
      <broker address="http://kafkaBroker5:9092"/>
  	  <broker address="http://kafkaBroker6:9092"/>
      <broker address="http://kafkaBroker7:9092"/>
      <broker address="http://kafkaBroker8:9092"/>
    </target>
  </targets>
  <rules>
    <logger writeTo="Kafka" minlevel="Trace" name="*"></logger>
  </rules>
</nlog>